pipeline {
    agent any

    stages{
        stage ('check dir abd make dir'){
            steps{
                sh 'ls -la'
                sh 'mkdir -p copy'
            }
        }
        stage('deploy to S3'){
            steps{
               sh 'aws s3 cp . s3://sanathwebpage --recursive'
               sh 'aws s3 cp s3://sanathwebpage/hello.txt copy/.'

            }
        }
        stage("read file"){
            steps{
                sh 'cat hello.txt'
            }
        }
    }

}
